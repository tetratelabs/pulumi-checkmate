{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "resolveJsonModule": true
    },
    "include": ["package.json"],
    "files": [
        "httpHealth.ts",
        "index.ts",
        "localCommand.ts",
        "provider.ts",
        "utilities.ts"
    ]
}
